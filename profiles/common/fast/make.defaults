# probably safe

_FLAGS="$_FLAGS -fbranch-target-load-optimize -fdelete-dead-exceptions -fstdarg-opt"
_FLAGS="$_FLAGS -fmodulo-sched -fmodulo-sched-allow-regmoves -freschedule-modulo-scheduled-loops"
CFLAGS_SMALL=" -fno-reschedule-modulo-scheduled-loops$CFLAGS_SMALL"
# keep here until new reasons to split
_FLAGS="$_FLAGS -fgcse-sm -fgcse-las -fvariable-expansion-in-unroller -fweb -frename-registers"

LDFLAGS="-Wl,-O1 -Wl,--as-needed -Wl,--hash-style=gnu -Wl,--sort-common -Wl,--sort-section=alignment"
