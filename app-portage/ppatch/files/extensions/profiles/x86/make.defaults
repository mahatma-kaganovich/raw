ACCEPT_KEYWORDS="amd64 ~amd64 x86 ~x86"
MAKEOPTS="-j2 -s"
CPU_FLAGS_X86="smp"
USE="$CPU_FLAGS_X86"
CFLAGS="$CFLAGS -mtune=generic -mfpmath=387 -momit-leaf-frame-pointer -mtls-dialect=gnu2 -minline-stringops-dynamically -maccumulate-outgoing-args -fira-loop-pressure"
FFLAGS="$FFLAGS $CFLAGS"
FCFLAGS="$FCFLAGS $CFLAGS"
CXXFLAGS="$CXXFLAGS $CFLAGS -fvisibility-inlines-hidden -fpermissive -fno-strict-aliasing -w"
