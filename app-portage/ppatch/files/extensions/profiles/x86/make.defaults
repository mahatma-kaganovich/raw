ACCEPT_KEYWORDS="amd64 ~amd64 x86 ~x86"
MAKEOPTS="-j2 -s"
CPU_FLAGS_X86="smp"
USE="$CPU_FLAGS_X86"
CFLAGS="$CFLAGS -mtune=generic -momit-leaf-frame-pointer -mtls-dialect=gnu2 -minline-stringops-dynamically -maccumulate-outgoing-args -fira-loop-pressure"
FFLAGS="$CFLAGS"
FCFLAGS="$CFLAGS"
CXXFLAGS="$CFLAGS"
CXXFLAGS="$CXXFLAGS -fvisibility-inlines-hidden"
