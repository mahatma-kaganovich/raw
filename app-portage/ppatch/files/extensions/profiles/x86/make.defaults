ACCEPT_KEYWORDS="amd64 ~amd64 x86 ~x86"
MAKEOPTS="-j2 -s"
CPU_FLAGS_X86="smp"
USE="$CPU_FLAGS_X86"

_FLAGS="$_FLAGS -mtune=generic -mfpmath=387 -momit-leaf-frame-pointer -mtls-dialect=gnu2 -minline-stringops-dynamically -maccumulate-outgoing-args -fira-loop-pressure"
CXXFLAGS="$CXXFLAGS -fvisibility-inlines-hidden -fpermissive -fno-strict-aliasing -w"

CFLAGS="$CFLAGS $_FLAGS"
FFLAGS="$FFLAGS $_FLAGS"
FCFLAGS="$FCFLAGS $_FLAGS"
CXXFLAGS="$CXXFLAGS $_FLAGS"
